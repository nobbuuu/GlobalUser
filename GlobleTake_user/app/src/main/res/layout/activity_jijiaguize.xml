<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:id="@+id/root_lay"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:background="@color/statecolor"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/back_relay"
            android:layout_width="@dimen/backimg_parent_width"
            android:layout_height="@dimen/backimg_parent_height">
            <ImageView
                android:layout_width="wrap_content"
                android:src="@mipmap/sousuo_xiangzuo"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/backimg_marginleft"
                android:layout_height="wrap_content" />
        </RelativeLayout>
        <TextView
            android:layout_width="wrap_content"
            android:text="计价规则"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:layout_centerInParent="true"
            android:layout_height="wrap_content" />
    </RelativeLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:background="@color/endOrderBg"
        android:layout_height="match_parent">
        <ImageView
            android:id="@+id/carphoto_iv"
            android:layout_width="wrap_content"
            android:layout_marginTop="152px"
            android:layout_gravity="center_horizontal"
            android:src="@mipmap/jijiaguize_car"
            android:layout_height="wrap_content" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:gravity="center_horizontal"
            android:layout_marginTop="96px"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:layout_width="100dp"
                android:background="@color/endOrder_fengexian"
                android:layout_gravity="center_vertical"
                android:layout_height="1dp"></RelativeLayout>
            <TextView
                android:layout_width="wrap_content"
                android:text="收费标准"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:textColor="@color/color999"
                android:layout_height="wrap_content" />
            <RelativeLayout
                android:layout_width="100dp"
                android:background="@color/endOrder_fengexian"
                android:layout_gravity="center_vertical"
                android:layout_height="1dp"></RelativeLayout>
        </LinearLayout>

        <TextView
            android:id="@+id/qibujia_tv"
            android:layout_width="wrap_content"
            android:text="4.0元/1200米起步价"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="63px"
            android:textColor="@color/color333"
            android:textSize="18sp"
            android:layout_height="wrap_content" />

        <TextView
            android:layout_width="wrap_content"
            android:text="+"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="22px"
            android:textColor="@color/color666"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/meterprice_tv"
            android:layout_width="wrap_content"
            android:text="4.0元/1200米起步价"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="22px"
            android:textColor="@color/color333"
            android:textSize="18sp"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/jia_tv"
            android:layout_width="wrap_content"
            android:text="+"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="22px"
            android:textColor="@color/color666"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/minuteprice_tv"
            android:layout_width="wrap_content"
            android:text="4.0元/1200米起步价"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="22px"
            android:textColor="@color/color333"
            android:textSize="18sp"
            android:layout_height="wrap_content" />
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="36px"
                android:background="@mipmap/jijiaguize_tishi"
                android:layout_gravity="bottom"
                android:layout_marginBottom="72px"
                android:layout_height="36px" />
            <TextView
                android:id="@+id/endorder_billingrule"
                android:layout_width="wrap_content"
                android:text="计价规则"
                android:textColor="@color/color666"
                android:gravity="bottom"
                android:layout_gravity="center_horizontal"
                android:textSize="18sp"
                android:layout_marginLeft="7dp"
                android:layout_marginBottom="72px"
                android:layout_height="match_parent" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
